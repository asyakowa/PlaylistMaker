<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BtnActivityMain" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:insetTop"> @dimen/dpfive</item>
        <item name="android:insetBottom"> @dimen/dpfive</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dptwo</item>

        <item name="android:layout_weight">@dimen/one</item>
        <item name="android:backgroundTint">@color/white</item>

        <item name="cornerRadius">@dimen/dpone</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/iconcolor</item>
        <item name="android:textSize">@dimen/spone</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">@color/iconcolor</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>


    <style name="BtnSettingsText" >

        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/sptwo</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="BtnSettings" parent="BtnSettingsText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">61dp</item>
        <item name="android:paddingStart">@dimen/dptwo</item>
        <item name="android:paddingEnd">@dimen/dptwo</item>
        <item name="android:background">@color/transparent</item>
        <item name="drawableTint">@color/grey</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="LLstyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>

        <item name="android:orientation">vertical</item>
    </style>
    <style name="LLstyleMain" parent="LLstyle" >
        <item name="android:background">@color/iconcolor</item>
    </style>
    <style name="LLstyleStns" parent="LLstyle" >
        <item name="android:background">@color/iconcolor</item>
    </style>

    <style name="MainTextinActivity">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/spone</item>
    </style>
    <style name="MainTextinActivityMain"   >
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/spone</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="MainTextinActivitySettings" parent="MainTextinActivity" >
        <item name="android:textColor">@color/white</item>

    </style>

    <style name="CustomSwitch" >
        <item name="android:trackTint">@color/lightblue</item>
        <item name="android:thumbTint">@color/blue</item>
    </style>

</resources>